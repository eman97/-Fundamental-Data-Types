/*
Write a program that prints a Â­Christmas tree:
   /\'
  /  \'
 /    \'
/      \'
--------
  "  "
  "  "
  "  "
Remember to use escape sequences.
*/

public class e4.19{
    public static void main(String[] args) {
        System.out.print("   /\\'  \n  /  \\'  \n /    \\' \n/      \\'\n");
        System.out.println("--------");
        System.out.println("  \"  \"  ");
        System.out.println("  \"  \"  ");
        System.out.println("  \"  \"  ");
    }
}
